{
  "name": "inmoapp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "prebuild": "yarn generate",
    "start": "next start",
    "lint": "next lint",
    "studio": "prisma studio",
    "generate": "prisma generate --schema=./prisma/schema.prisma",
    "db:push": "prisma db push",
    "db:seed": "ts-node --esm prisma/seeders/shared-seed.ts && ts-node --esm prisma/seeders/tenant-seed.ts",
    "db:reset": "prisma db push --force-reset && yarn db:seed",
    "db:setup": "yarn db:push && yarn db:seed",
    "prisma-initialize": "(yarn generate) && (yarn db:reset) && (yarn db:seed)",
    "generate:push": "(yarn generate) && (yarn db:push)",
    "type-check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.782.0",
    "@aws-sdk/s3-request-presigner": "^3.782.0",
    "@hookform/resolvers": "^5.0.1",
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-tabs": "^1.1.4",
    "@radix-ui/react-toast": "^1.2.6",
    "@types/bcryptjs": "^3.0.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.487.0",
    "mercadopago": "^2.4.0",
    "next": "15.2.4",
    "next-auth": "^4.24.11",
    "prisma": "^6.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.55.0",
    "tailwind-merge": "^3.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^29.5.14",
    "@types/mercadopago": "^1.5.11",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
