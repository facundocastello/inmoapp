{
  "name": "multi-tenant",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "studio": "prisma studio",
    "tenant-studio": "prisma studio --schema=./prisma/tenant-schema.prisma",
    "generate": "prisma generate --schema=./prisma/schema.prisma",
    "generate:tenant": "prisma generate --schema=./prisma/tenant-schema.prisma",
    "db:push": "prisma db push",
    "db:push:tenant": "prisma db push --schema=./prisma/tenant-schema.prisma",
    "db:seed": "ts-node --esm prisma/seeders/shared-seed.ts",
    "db:seed:tenant": "ts-node --esm --project tsconfig.node.json prisma/seeders/tenant-seed.ts",
    "db:reset": "prisma db push --force-reset && yarn db:seed",
    "db:reset:tenant": "prisma db push --force-reset --schema=./prisma/tenant-schema.prisma && yarn db:seed:tenant",
    "db:setup": "yarn db:push && yarn db:push:tenant && yarn db:seed && yarn db:seed:tenant",
    "db:studio": "prisma studio & prisma studio --schema=./prisma/tenant-schema.prisma --port=5556",
    "initialize": "(yarn generate & yarn generate:tenant) && (yarn db:reset & yarn db:reset:tenant) && (yarn db:seed & yarn db:seed:tenant)",
    "type-check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "@types/bcryptjs": "^3.0.0",
    "bcryptjs": "^3.0.2",
    "next": "15.2.4",
    "prisma": "^6.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "prettier": "^3.2.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
